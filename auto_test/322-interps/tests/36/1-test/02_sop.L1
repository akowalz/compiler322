((
  ;; <<
  (edi <- 3)
  (edi <<= 1)
  (edi *= 2)
  (edi += 1)
  (eax <- (print edi)) ;; print 6
  
  (edi <- 3)
  (ecx <- 1)
  (edi <<= ecx)
  (edi *= 2)
  (edi += 1)
  (eax <- (print edi)) ;; print 6
  
  ;; >>
  (edi <- 7)
  (edi >>= 1)
  (edi *= 2)
  (edi += 1)
  (eax <- (print edi)) ;; print 3
  
  (edi <- 7)
  (ecx <- 1)
  (edi >>= ecx)
  (edi *= 2)
  (edi += 1)
  (eax <- (print edi)) ;; print 3
  ))